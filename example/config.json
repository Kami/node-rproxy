{

  "server": {
    "host": "localhost",
    "port": 8000,
    "secure": false
  },

  "target": {
    "host": "localhost",
    "port": 8787,
    "secure": false,
    "middleware_run_list": ["identity_provider", "rate_limiting",
                            "authentication"]
  },

  "database": {
    "host": "localhost",
    "port": 6379,
    "password": null
  },

  "middleware": {
    "identity_provider": {},
     "rate_limiting": {
        "depends": ["identity_provider"]
    },
    "authentication": {
      "depends": ["identity_provider"]
    }
  },

  "rate_limits": [
    {
      "method": "GET",
      "path_regex": "/.*?",
      "limit": 10,
      "period": 3600
    }
  ]
}
